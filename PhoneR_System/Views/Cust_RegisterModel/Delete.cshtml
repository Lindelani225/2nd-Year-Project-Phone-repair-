@model PhoneR_System.Models.Cust_RegisterModel

@{
    ViewBag.Title = "Delete";
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">


    <meta content="" name="descriptison">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="~/Content/assets/img/favicon.png" rel="icon">
    <link href="~/Content/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/Content/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/icofont/icofont.min.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/venobox/venobox.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">

    <link href="~/Content/assets/css/style.css" rel="stylesheet">




</head>
<body>
    <header id="header" class="fixed-top header-inner-pages">
        <div class="container-fluid">
            <div>
                <div class="col-xl-9 d-flex align-items-center justify-content-between">
                    <a href="#" class="logo"><img src="~/Content/assets/img/logo.png" alt="" class="img-fluid"></a>
                    <h1 class="logo"><a href="#">Shadrak's Repair</a></h1>

                    <a href="index.html" class="logo"><img src="assets/img/logo.png" alt="" class="img-fluid"></a>

                    <nav class="nav-menu d-none d-lg-block">
                        <ul>
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li class="drop-down">
                                <a href="">Services</a>
                                <ul>
                                    <li>@Html.ActionLink("Request Pick-Up Device", "Index", "Cust_Request")</li>
                                    <li>@Html.ActionLink("Book of Walk-In Appointment", "Index", "Cust_WalkIn")</li>

                                </ul>
                            </li>
                            <li>@Html.ActionLink("About", "Index", "Cust_Request")</li>
                            <li>@Html.ActionLink("Contact", "Index", "Cust_Request")</li>

                        </ul>
                    </nav><!-- .nav-menu -->

                    @Html.ActionLink("Log out", "Index", "Home","1", htmlAttributes: new { @class = "get-started-btn scrollto" })
                </div>
            </div>

        </div>
        <style>
            .Em a {
                color: green;
            }
        </style>

    </header><!-- End Header -->
    <div class="form-horizontal" style="margin-top:7.5em;font-size:110%">
        <hr />
        <h2>Delete</h2>

        <h3>Are you sure you want to delete this?</h3>
        <hr />
        <div>
            <h4>Register</h4>
            <hr />
            <dl class="dl-horizontal Em">
                <dt>
                    @Html.DisplayNameFor(model => model.FtName)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.FtName)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.LastName)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.LastName)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Phone)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Phone)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Email)
                </dt>

                <dd>
                    <a>@Html.DisplayFor(model => model.Email)</a> 
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Pass)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Pass)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Role.Rol)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Role.Rol)
                </dd>

            </dl>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-actions no-color">
                    <input type="submit" value="Delete" class="btn btn-warning" /> |
                    @Html.ActionLink("Back to List", "Index","Cust_RegisterModel",htmlAttributes:new { @style="color:green"})
                </div>
            }
        </div>
</div>
</body>
</html>